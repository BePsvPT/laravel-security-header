{
  "name": "bepsvpt/laravel-security-header",
  "description": "Append security headers to response for laravel framework.",
  "keywords": ["laravel", "header", "hsts", "hpkp", "https"],
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/http": "5.1.*|5.2.*|5.3.*",
    "illuminate/support": "5.1.*|5.2.*|5.3.*",
    "paragonie/csp-builder": "^2.0",
    "paragonie/hpkp-builder": "^0.1"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "~0.3",
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Bepsvpt\\LaravelSecurityHeader\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/helpers.php"
    ]
  }
}
