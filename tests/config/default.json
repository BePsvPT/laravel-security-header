{
  "x_content_type_options": "nosniff",
  "x_frame_options": "sameorigin",
  "x_xss_protection": "1; mode=block",
  "hsts": {
    "enable": false,
    "max_age": 15552000,
    "include_sub_domains": false
  },
  "hpkp": {
    "hashes": [
      { "algo": "sha256", "hash": "your-hash" }
    ],
    "include-subdomains": false,
    "max-age": 2592000,
    "report-only": false,
    "report-uri": null
  },
  "csp": {
    "report-only": false,
    "base-uri": [],
    "default-src": [],
    "child-src": [],
    "connect-src": {
      "allow": [],
      "self": false
    },
    "font-src": {
      "allow": [],
      "self": false
    },
    "form-action": {
      "allow": [],
      "self": false
    },
    "frame-ancestors": [],
    "img-src": {
      "allow": [],
      "types": [],
      "self": false,
      "data": false
    },
    "media-src": [],
    "object-src": [],
    "plugin-types": [],
    "script-src": {
      "allow": [],
      "hashes": [],
      "self": false,
      "unsafe-inline": false,
      "unsafe-eval": false
    },
    "style-src": {
      "allow": [],
      "self": false,
      "unsafe-inline": false
    },
    "upgrade-insecure-requests": false
  }
}
